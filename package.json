{
  "name": "sullivan-scott",
  "version": "1.0.0",
  "description": "scott sullivan information website",
  "private": true,
  "main": "server.js",
  "scripts": {
    "--------- START ------------------------------": "",
    "start": "yarn copy:assets && NODE_ENV=development webpack-dev-server",
    "start:prod": "NODE_ENV=production yarn build && yarn serve",
    "build": "webpack && yarn copy:assets",
    "build:prod": "NODE_ENV=production  webpack && yarn copy:assets",
    "serve": "node ./server.js",
    "--------- STORYBOOK ------------------------------": "",
    "storybook": "start-storybook -p 9003",
    "build:storybook": "build-storybook -o build/storybook",
    "--------- LINTING ----------------------------": "",
    "lint": "yarn lint:scss && yarn lint:js",
    "lint:scss": "stylelint 'src/**.scss' --ignore-path .eslintignore",
    "lint:js": "eslint --ext .jsx --ext .js src/ cypress/",
    "prettier": "prettier --write \"**/*.{json,md,js,jsx,css,scss}\"",
    "prettier:diff": "prettier --list-different \"**/*.{json,md,js,jsx,css,scss}\"",
    "--------- BUNDLE CHECKS ----------------------": "",
    "bundle:size": "NODE_ENV=production yarn build && bundlesize",
    "bundle:budget": "yarn build && start-server-and-test serve http-get://localhost:3001/ bundle:budget:run",
    "bundle:budget:run": "yarn lighthouse http://localhost:3001/ --budget-path=config/performance/lighthouse-budgets.json --output-path=reports/lighthouse-results.html --view",
    "--------- UNIT TESTS ------------------------------": "",
    "test:unit": "jest --coverage --config ./config/jest/jest.config.js",
    "test:unit:watch": "jest --coverage --watch --config ./config/jest/jest.config.js",
    "--------- E2E TESTS ------------------------------": "",
    "test:e2e": "NODE_ENV=development yarn build && start-server-and-test serve http-get://localhost:3001/ cypress:open",
    "test:e2e:run": "NODE_ENV=development yarn build && start-server-and-test serve http-get://localhost:3001/ cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "percy exec -- cypress run",
    "--------- HELPERS ----------------------------": "",
    "check:status": "node -e 'require(\"./config/helpers/nodeScripts\").statusCheck()' && node -e 'require(\"./config/helpers/nodeScripts\").branchCheck()'",
    "copy:assets": "node -e 'require(\"./config/helpers/nodeScripts\").moveAssets()'",
    "badges": "yarn test:unit --silent && jest-coverage-badges output './src/assets'",
    "prepush:hook": "yarn check:status && yarn test:unit && yarn test:e2e:run && yarn bundle:size",
    "nuke": "yarn cache clean && yarn global add rimraf && rimraf node_modules && yarn && yarn run clean"
  },
  "lint-staged": {
    "*.{js,jsx,css,md}": [
      "yarn prettier",
      "yarn lint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn prepush:hook"
    }
  },
  "engines": {
    "node": ">=10",
    "npm": ">=5"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express-static-gzip": "^2.0.5",
    "framer-motion": "^1.6.12",
    "lodash.throttle": "^4.1.1",
    "prop-types": "15.7.2",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.6.4"
