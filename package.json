    "ts-jest": "^24.0.2",
    "webpack": "^4.39.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.8.2",
    "webpack-merge": "4.2.2"
  },
  "bundlesize": [
    {
      "path": "./build/js/main.*.js",
      "maxSize": "5 kB"
    },
    {
      "path": "./build/js/vendors.*.js",
      "maxSize": "26.91 kB"
    },
    {
      "path": "./build/css/main.*.css",
      "maxSize": "2.2 kB"
    }
  ]
}
